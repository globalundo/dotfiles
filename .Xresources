
#include ".Xresources.d/urxvt"

!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------

Xft.dpi:       96
Xft.antialias: true
Xft.rgba:      rgb
Xft.hinting:   true
Xft.hintstyle: hintslight
Xft.autohint:  0
Xft.lcdfilter: lcddefault

!-------------------------------------------------------------------------------
! Rofi settings
!-------------------------------------------------------------------------------

rofi.terminal:     urxvt
rofi.width:        60%
rofi.sidebar-mode: false
rofi.fuzzy:        true

